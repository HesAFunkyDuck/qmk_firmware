# My Build Options
#   change yes to no to disable
#   This does overwrite options from the main rules file.

MOUSEKEY_ENABLE = no       # Mouse keys
CONSOLE_ENABLE = no        # Console for debug (Only enable for debuging)
COMMAND_ENABLE = no        # Commands for debug and configuration (Only enable for debuging)

LTO_ENABLE = yes           # This will save the most space but will cause a loger compile time. After testing the compile is about the same.
KEY_LOCK_ENABLE = yes      # Enable Locking Keys
SPACE_CADET_ENABLE = no	   # Disable Space Cadet, this would be useful if I was coding
GRAVE_ESC_ENABLE = no 	   # Disable Grave Escape Key, I am not using this
MAGIC_ENABLE = no
COMBO_ENABLE = yes
